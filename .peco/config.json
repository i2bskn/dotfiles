{
    "Keymap": {
        "C-k": "peco.SelectUp",
        "C-j": "peco.SelectDown",
        "C-f": "peco.ScrollPageDown",
        "C-b": "peco.ScrollPageUp",
        "C-u": "peco.DeleteAll",
        "C-c": "peco.Cancel"
    },
    "Style": {
        "Basic": ["on_default", "default"],
        "Selected": ["bold", "on_blue", "default"],
        "Query": ["default", "bold"],
        "Matched": ["cyan", "on_default"]
    },
    "Prompt": "[peco]"
}
